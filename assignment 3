nu=[]
def add(u):
        s=sum(u)
        return(s)
def sub(num1,num2):
    return (num1-num2)
def mul(num1,num2):
    return (num1*num2)
def qu(num1,num2):
    return (num1/num2)
def re(num1,num2):
    return (num1%num2)
def av(u):
    return ((sum(u))/len(u))
def per(u):
    return (((sum(u))/(len(u)*100))*100)
def po(num1,num2):
    return (num1**num2)
def ro(num1,num2):
    return ((num1)**(1/num2))

print("Enter the operation you want to do :")
print("1.Addition")
print("2.Substration")
print("3.multiplication")
print("4.Quotient")
print("5.Remainder")
print("6.Average")
print("7.Percentage")
print("8.power")
print("9.under root")
i=int(input("INPUT YOUR CHOICE"))

if i==1:
    l=int(input("enter the total number you want to add :"))
    for i in range(0,l):
        h=int(input("enter the number to want to add"))
        nu.append(h)
    print(add(nu))
elif i==2:
    c=int(input("enter the number to want to substract from"))
    d=int(input("enter the number you want to make the substract it"))
    print(sub(c,d))
elif i==3:
     c=int(input("enter the number to want to multiply it"))
     d=int(input("enter the another number you want to multiply"))
     print(mul(c,d))
elif i==4:
    c=int(input("enter the number to want to divide from"))
    d=int(input("enter the number to divide")) 
    print(qu(c,d))
elif i==5:
    c=int(input("enter the number to want to quotient "))
    d=int(input("enter the number from we get it"))
    print(re(c,d))
elif i==6:
    l=int(input("enter the total number you want take the average : :"))
    for i in range(0,l):
        h=int(input("enter the number to want to add"))
        nu.append(h) 
    print(av(nu))
elif i==7:
    l=int(input("enter the total number you want to take the percentage :"))
    for i in range(0,l):
        h=int(input("enter the number to want to add"))
        nu.append(h)
    print(per(nu))
    
elif i==8:
    c=int(input("enter the number to want to power it"))
    d=int(input("enter the power you want to make"))
    print(po(c,d))
elif i==9:
    c=int(input("enter the number to want to root it"))
    d=int(input("enter the root value you want to make"))
    print(ro(c,d))
 
elif i==10:
    pass
